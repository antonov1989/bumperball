header {
  background: url("../../assets/img/bg-header.png") no-repeat top center;
  position: relative;
  z-index: 200;
}
header .header-inner {
  position: relative;
}
header .header-top {
  background: #212735;
  position: absolute;
  z-index: 100;
  width: 100%;
}
header .header-top:after {
  content: "";
  position: absolute;
  top: 0;
  left: -11px;
  height: 40px;
  width: 40px;
  background: #212735;
  -webkit-transform: skew(30deg, 0);
  -moz-transform: skew(30deg, 0);
  -ms-transform: skew(30deg, 0);
  -o-transform: skew(30deg, 0);
  z-index: -1;
}
header .header-top:before {
  content: "";
  position: absolute;
  top: 0;
  right: -11px;
  height: 40px;
  width: 40px;
  background: #212735;
  -webkit-transform: skew(-30deg, 0);
  -moz-transform: skew(-30deg, 0);
  -ms-transform: skew(-30deg, 0);
  -o-transform: skew(-30deg, 0);
  z-index: -1;
}
header .header-top .navbar-nav a {
  color: #fff;
  font-size: 12px;
  padding: 10px 15px;
  -moz-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  -webkit-transition: all 0.3s ease;
  transition: all 0.3s ease;
