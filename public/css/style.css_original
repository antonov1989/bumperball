  width: 100%;
}
.lp-page .grid-qd .grid-row .cell {
  display: table-cell;
  vertical-align: middle;
  text-align: center;
}
.lp-page .grid-qd .grid-row.line-one .cell {
  width: 33.333%;
}
.lp-page .grid-qd .grid-row.line-one .cell:first-child {
  text-align: right;
}
.lp-page .grid-qd .grid-row.line-one .cell:first-child img {
  position: relative;
  bottom: -88px;
  right: -58px;
}
.lp-page .grid-qd .grid-row.line-one .cell:nth-child(2) img {
  position: relative;
  bottom: -67px;
}
.lp-page .grid-qd .grid-row.line-one .cell:last-child {
  text-align: left;
}
.lp-page .grid-qd .grid-row.line-one .cell:last-child img {
  position: relative;
  bottom: -55px;
}
.lp-page .grid-qd .grid-row.line-two .cell {
  width: 25%;
}
.lp-page .multi-grid {
  margin: 40px 0;
  position: relative;
}
@media (min-width: 992px) {
  .lp-page .multi-grid {
    margin-left: -15px;
  }
  .lp-page .multi-grid .box-btn {
    text-align: center;
    padding: 0;
    position: absolute;
    right: 100%;
    width: 100%;
    bottom: 30px;
    padding-right: 20px;
  }
  .lp-page .multi-grid .box-btn .btn-main {
    float: right;
  }
}
@media (min-width: 768px) and (max-width: 991px) {
  .lp-page .multi-grid {
    margin-left: -15px;
  }
}
.lp-page .descr-category {
  margin: 40px 0;
}
.lp-page .descr-category .image {
  margin-bottom: 30px;
}
.lp-page .descr-category .image img {
  width: 100%;
}
.lp-page .descr-category .text-box {
  color: #616161;
}
.lp-page .grid-accessories .square-box-title {
  background: #009fc7;
}
.lp-vape .video-bg {
  width: 100%;
  height: 700px;
}
@media (max-width: 767px) {
  .lp-vape .video-bg {
    height: 300px;
  }
}
.lp-vape .video-bg .txt {
  text-transform: uppercase;
  color: #fff;
  font-size: 44px;
  font-family: 'Ubuntu Condensed';
  height: 100%;
  padding-top: 252px;
}
@media (max-width: 767px) {
  .lp-vape .video-bg .txt {
    padding-top: 213px;
    font-size: 31px;
  }
}
.lp-vape .square-box-title p {
  text-align: center;
}
.lp-vape .intro {
  height: 217px;
}
.lp-vape .description-product {
  text-align: center;
}
.lp-vape .description-product .title-block {
  font-size: 26px;
  color: #009fc7;
  font-weight: 300;
  text-transform: uppercase;
  font-family: 'Ubuntu Condensed';
  margin-bottom: 36px;
}
.lp-vape .point-block {
  background: #fff;
  position: relative;
  z-index: 100;
}
.lp-vape .point-block .header-point {
  text-align: center;
}
.lp-vape .point-block .header-point .number {
  display: inline-block;
  background: #00bfef;
  color: #fff;
  width: 50px;
  height: 50px;
  font-family: 'Ubuntu Condensed';
  font-size: 36px;
  text-align: center;
  padding-left: 5px;
  line-height: 50px;
}
.lp-vape .point-block .header-point .title {
  font-family: 'Ubuntu Condensed';
  font-weight: bold;
  color: #000000;
  font-size: 26px;
  text-transform: uppercase;
  padding: 30px 0;
}
@media (max-width: 767px) {
  .lp-vape .point-block .header-point .title {
    padding: 16px 0;
    font-size: 25px;
  }
}
.lp-vape .point-block .header-point .descr {
  font-size: 16px;
  color: #696969;
}
.lp-vape .point-block .inner {
  position: relative;
  margin-top: 20px;
  padding-bottom: 100px;
}
@media (max-width: 767px) {
  .lp-vape .point-block .inner {
    padding-bottom: 23px;
  }
}
.lp-vape .point-block .inner:after {
  content: "";
  position: absolute;
  top: 0;
  left: 50%;
  width: 2px;
  height: 0;
  margin-left: -1px;
  background: #00bfef;
  -webkit-transition: height 1.5s ease-out;
  transition: height 1.5s ease-out;
}
.lp-vape .point-block .inner.scrollimation.in:after {
  height: 100%;
}
.lp-vape .point-block .inner .content {
  overflow: hidden;
}
.lp-vape .point-block .inner .content .image img {
  width: 100%;
}
.lp-vape .point-block .inner .content .txt {
  font-size: 16px;
  color: #696969;
  line-height: 25px;
  font-weight: 400;
}
@media (max-width: 767px) {
  .lp-vape .point-block .inner .content .txt {
    font-size: 11px;
    line-height: 18px;
  }
}
.lp-vape .point-block .inner .content .txt .cell {
  display: table-cell;
  vertical-align: middle;
}
@media (max-width: 767px) {
  .lp-vape .point-block .inner .content .txt .cell {
    vertical-align: top;
  }
}
.lp-vape .point-block .inner .content .txt .cell-icon {
  width: 70px;
  text-align: center;
  padding-top: 0;
  vertical-align: top;
}
@media (max-width: 767px) {
  .lp-vape .point-block .inner .content .txt .cell-icon {
    width: 36px;
    text-align: center;
    padding-top: 13px;
  }
}
.lp-vape .point-block .inner .content .txt .cell-icon i {
  font-size: 50px;
}
@media (max-width: 767px) {
  .lp-vape .point-block .inner .content .txt .cell-icon i {
    font-size: 24px;
  }
}
.lp-vape .point-block .inner .content .txt .fa-times {
  color: #fa604b;
}
.lp-vape .point-block .inner .content .txt .fa-check {
  color: #77dc3a;
}
.lp-vape .point-block .inner .content.left-content .txt {
  text-align: right;
}
#darkness {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 199;
  background-color: #212735;
  display: none;
  opacity: 0;
}
#darkness.active {
  opacity: 0.85;
  display: block;
}
.alert-ok .sweet-alert {
  position: relative;
  margin-top: 0!important;
}
.alert-ok .sweet-alert:focus {
  outline: none;
}
