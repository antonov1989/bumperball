.iradio2 {
  vertical-align: top;
  width: 17px;
  height: 17px;
  margin: 0 3px 0 0;
}
.iradio2 + span {
  cursor: pointer;
}
.iradio2:not(checked) {
  position: absolute;
  opacity: 0;
}
.iradio2:not(checked) + span {
  position: relative;
  padding: 0 0 0 28px;
}
.iradio2:not(checked) + span:before {
  content: '';
  position: absolute;
  top: 2px;
  left: 0;
  width: 16px;
  height: 16px;
  box-shadow: none;
  background: transparent;
  border: 1px solid #919191;
}
