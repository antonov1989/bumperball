.chevron {
  position: relative;
  text-align: center;
  padding: 15px 39px;
  text-transform: uppercase;
  width: 261px;
  margin: 0 auto;
  font-size: 22px;
  line-height: 24px;
  color: #009fc7;
  vertical-align: middle;
  display: table-cell;
}
@media (min-width: 992px) and (max-width: 1199px) {
  .chevron {
    padding: 12px 14px;
    width: 143px;
    font-size: 15px;
    line-height: 19px;
  }
}
@media (min-width: 768px) and (max-width: 991px) {
  .chevron {
    padding: 0px 4px;
    width: 112px;
    font-size: 14px;
    line-height: 19px;
  }
}
@media (max-width: 767px) and (min-width: 480px) {
  .chevron {
    padding: 15px 5px;
    width: 30%;
    font-size: 14px;
    line-height: 16px;
  }
}
.chevron:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 51%;
  background: transparent;
  border: 1px solid #009fc7;
  border-right: transparent;
  -webkit-transform: skew(0deg, 8deg);
  -moz-transform: skew(0deg, 8deg);
  -ms-transform: skew(0deg, 8deg);
  -o-transform: skew(0deg, 8deg);
  transform: skew(0deg, 8deg);
  z-index: -1;
}
.chevron:after {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  height: 100%;
  width: 50%;
  background: transparent;
  border: 1px solid #009fc7;
  border-left: transparent;
  -webkit-transform: skew(0deg, -8deg);
  -moz-transform: skew(0deg, -8deg);
  -ms-transform: skew(0deg, -8deg);
  -o-transform: skew(0deg, -8deg);
  transform: skew(0deg, -8deg);
  z-index: -1;
}
.advertising {
  margin-bottom: 50px;
}
.advertising img {
  height: auto;
  max-width: 100%;
  width: 100%;
}
.advertising a {
  position: relative;
  display: block;
}
.advertising a:after {
  overflow: hidden;
  position: absolute;
  top: 0;
  content: "";
  z-index: 100;
  width: 100%;
  height: 100%;
  left: 0;
  opacity: 0;
  pointer-events: none;
  -webkit-transition: all 0.3s ease 0s;
  -o-transition: all 0.3s ease 0s;
  transition: all 0.3s ease 0s;
  background-color: rgba(255, 255, 255, 0.3);
  -webkit-transform: scale(0);
  -ms-transform: scale(0);
  transform: scale(0);
}
.advertising a:hover:after {
  visibility: visible;
  opacity: 0.8;
  -webkit-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
}
.btn-main {
  border-radius: 0;
  box-shadow: none;
  color: #009fc7;
  border: 1px solid #009fc7;
  font-size: 14px;
  text-transform: uppercase;
  padding: 4px 24px;
  float: right;
  position: relative;
}
.btn-main:before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  bottom: 0;
  z-index: 0;
  display: block;
  padding: 0;
  -webkit-transform: scale(1, 0);
  -moz-transform: scale(1, 0);
  -ms-transform: scale(1, 0);
  -o-transform: scale(1, 0);
  transform: scale(1, 0);
  -webkit-transform-origin: center top;
  transform-origin: center top;
  -webkit-transition: all 0.25s ease-out;
  -moz-transition: all 0.25s ease-out;
  -ms-transition: all 0.25s ease-out;
  -o-transition: all 0.25s ease-out;
  transition: all 0.25s ease-out;
  background: #009fc7;
}
.btn-main span {
  position: relative;
  z-index: 1;
  -webkit-transition: color .25s;
  -moz-transition: color .25s;
  -ms-transition: color .25s;
  -o-transition: color .25s;
  transition: color .25s;
}
.btn-main:hover {
  color: #fff;
}
.btn-main:hover:before {
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
  transform: scale(1);
}
.btn-main.grey {
